{
  "name": "tyler-element",
  "description": "仿elementUI的学习项目，提供一些基础组件",
  "version": "0.1.3",
  "author": "tyler Zheng",
  "license": "MIT",
  "keywords": ["VUE3", "Component", "UI"],
  "homepage": "https://github.com/tyler4400",
  "repository": "https://github.com/tyler4400/my-element",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": ["./dist/es/tyler-element.css", "./dist/umd/tyler-element.css"],
  "types": "./dist/types/index.d.ts",
  "main": "./dist/tyler-element.umd.cjs",
  "module": "./dist/tyler-element.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/tyler-element.js",
      "require": "./dist/umd/tyler-element.umd.cjs"
    },
    "./style.css": "./dist/es/tyler-element.css"
  },
  "scripts": {
    "dev": "vite",
    "build-theck": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "build": "run-p build-es build-umd",
    "build-es": "vite build --config vite.es.config.ts",
    "build-umd": "vite build --config vite.umd.config.ts",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.5",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@vue/test-utils": "^2.4.6",
    "async-validator": "^4.2.5",
    "axios": "^1.7.9",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.3",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "typescript": "~5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vitepress": "^1.5.0",
    "vitest": "^2.1.8",
    "vue": "^3.5.13",
    "vue-tsc": "^2.1.10"
  }
}
